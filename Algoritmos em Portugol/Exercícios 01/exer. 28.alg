algoritmo "semnome"
var
    valor1, valor2, valor3, soma: real
inicio
    escreva("Digite o 1o valor: ")
    leia(valor1)
    escreva("Digite o 2o valor: ")
    leia(valor2)
    escreva("Digite o 3o valor: ")
    leia(valor3)
    soma <- valor1 ^ 2 + valor2 ^ 2 + valor3 ^ 2
    escreval("A soma de ", valor1, ",", valor2, ",", valor3, ", elevado ao quadrado eh ", soma)
fimalgoritmo
