algoritmo "semnome"
var
    nota1, nota2, nota3, nota4, soma: real
inicio
    escreva("Digite a 1a nota: ")
    leia(nota1)
    escreva("Digite a 2a nota: ")
    leia(nota2)
    escreva("Digite a 3a nota: ")
    leia(nota3)
    escreva("Digite a 4a nota: ")
    leia(nota4)
    soma <- (nota1 + nota2 + nota3 + nota4)/4
    escreval("A soma de todas as notas foram: ", soma)
fimalgoritmo
