algoritmo "verdadeiroefalso"
var
 V, F: logico
inicio
    V <- verdadeiro
    F <- falso
    escreval("  E:")
    escreval(V e V)
    escreval(F e F)
    escreval(V e F)
    escreval(F e V)
    
    escreval("  OU:")
    escreval(V ou V)
    escreval(F ou V)
    escreval(V ou F)
    escreval(F ou F)
fimalgoritmo
